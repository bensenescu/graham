{
  "name": "graham",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "preview": "npm run build && vite preview --port 3001",
    "deploy": "npm run db:migrate:prod && npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "types:check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:write": "prettier . --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "drizzle-kit migrate",
    "db:migrate:prod": "npx everyapp app remote-d1-shell -- drizzle-kit migrate --config=drizzle-prod.config.ts",
    "db:studio:local": "drizzle-kit studio",
    "db:studio:prod": "npx everyapp app remote-d1-shell -- drizzle-kit studio --config=drizzle-prod.config.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.13",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@every-app/sdk": "^0.1.5",
    "@tanstack/query-core": "^5.90.9",
    "@tanstack/query-db-collection": "^1.0.0",
    "@tanstack/query-sync-storage-persister": "^5.90.14",
    "@tanstack/react-db": "^0.1.44",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "@tanstack/react-router": "^1.136.3",
    "@tanstack/react-router-devtools": "^1.136.3",
    "@tanstack/react-start": "^1.136.3",
    "@tiptap/extension-collaboration": "^3.18.0",
    "@tiptap/extension-collaboration-caret": "^3.18.0",
    "@tiptap/extension-placeholder": "^3.18.0",
    "@tiptap/react": "^3.18.0",
    "@tiptap/starter-kit": "^3.18.0",
    "ai": "^6.0.44",
    "cloudflare": "^5.2.0",
    "daisyui": "^5.5.5",
    "drizzle-orm": "^0.44.4",
    "jose": "^6.0.12",
    "lib0": "^0.2.117",
    "lucide-react": "^0.542.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^5.2.3",
    "react-markdown": "^10.1.0",
    "remeda": "^2.33.4",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.16",
    "y-protocols": "^1.0.7",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.29",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.18",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@libsql/client": "^0.15.15",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.18.13",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "drizzle-kit": "^0.31.4",
    "knip": "^5.66.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.45.3"
  }
